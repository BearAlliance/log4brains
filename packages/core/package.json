{
  "name": "@log4brains/core",
  "version": "0.0.0",
  "description": "Log4brains core API",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:log4brains/log4brains.git",
  "author": "Thomas Vaillant <vaillant.thomas+gh@gmail.com>",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "dev": "microbundle --format es,cjs watch",
    "build": "microbundle --format es,cjs",
    "clean": "rimraf ./dist",
    "typescript": "tsc --noEmit",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "eslint . --ext ts --ignore-pattern '/out/' --max-warnings=0",
    "lint-fix": "eslint . --ext ts --ignore-pattern '/out/' --max-warnings=0 --fix",
    "typedoc": "typedoc --mode library --includeVersion --readme none --theme minimal --out docs/typedoc src/index.ts"
  },
  "dependencies": {
    "awilix": "^4.2.6",
    "cheerio": "^1.0.0-rc.3",
    "joi": "^17.2.1",
    "lodash": "^4.17.20",
    "markdown-it": "^11.0.1",
    "neverthrow": "^2.7.1",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.22",
    "@types/joi": "^14.3.4",
    "@types/lodash": "^4.14.161",
    "@types/markdown-it": "^10.0.2",
    "microbundle": "^0.12.4"
  }
}
