{
  "name": "@log4brains/web",
  "version": "0.0.0",
  "description": "Preview and publish your Architecture Decision Records (ADR) with log4brains",
  "repository": "git@github.com:log4brains/log4brains.git",
  "author": "Thomas Vaillant <vaillant.thomas+gh@gmail.com>",
  "license": "Apache-2.0",
  "private": false,
  "bin": {
    "log4brains-web": "./dist/bin/log4brains-web"
  },
  "scripts": {
    "dev": "cross-env LOG4BRAINS_CWD=../.. LOG4BRAINS_DEV=1 ESM_DISABLE_CACHE=true node -r esm -r ts-node/register ./src/bin/main.ts",
    "next": "cross-env LOG4BRAINS_CWD=../.. next",
    "build": "tsc --noEmit false && copyfiles next.config.js dist && copyfiles -u 1 src/bin/log4brains-web dist",
    "clean": "rimraf ./dist",
    "typescript": "tsc",
    "lint": "eslint . --ext ts,tsx --ignore-pattern '/out/' --max-warnings=0",
    "lint-fix": "eslint . --ext ts,tsx --ignore-pattern '/out/' --max-warnings=0 --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@log4brains/core": "0.0.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "chalk": "^4.1.0",
    "clsx": "^1.1.1",
    "commander": "^6.1.0",
    "esm": "^3.2.25",
    "markdown-to-jsx": "^7.0.1",
    "next": "9.5.3",
    "next-transpile-modules": "^4.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "signale": "^1.4.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/react": "^16.9.49",
    "@types/signale": "^1.4.1",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.34",
    "@types/url-parse": "^1.4.3",
    "babel-loader": "^8.1.0",
    "copyfiles": "^2.4.0",
    "react-is": "^16.13.1",
    "ts-node": "^9.0.0"
  }
}
